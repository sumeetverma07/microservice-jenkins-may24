pipeline {
    agent any
    tools {
        python3 "python"
    }
    stages {
    
            stage('Git Checkout') {
             steps {
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'docker-hub', url: 'https://github.com/sumeetverma07/microservice-jenkins-may24.git']])                }
            }
        }
    }
